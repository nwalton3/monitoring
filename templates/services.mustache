<h2>{{pageTitle}}</h2>
<a id="btn-reload-services" class="btn btn-primary reload-all" >Reload now</a>

<table class="table table-condensed">
  <thead>
    <th>Service</th>
    <th>URL</th>
    <th>Status</th>
  </thead>
  <tbody>
  {{#each services}}
  <tr class="{{status}}">
    <td class="title">{{title}}</td>
    <td class="url">{{requestUrl}}</td>
    <td class="status">{{status}}</td>
  </tr>
  {{/each}}
  </tbody>
</table>