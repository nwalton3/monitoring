<h2>{{pageTitle}}</h2>
<a id="btn-reload-times" class="btn btn-primary reload-all" >Reload All</a>

<table class="table table-hover table-condensed">
  <thead>
    <th>Service</th>
    <th>Service URL</th>
    <th>Server</th>
    <th>Load Time (ms)</th>
    <th>Status</th>
    <th>Reload</th>
  </thead>
  <tbody>
  {{#each services}}
  <tr class="{{status}}">
    <td class="title"><a rel="popover" data-content="{{desc}}" data-original-title="{{title}}" data-trigger="manual">{{title}}</a></td>
    <td class="url">{{requestUrl}}</td>
    <td class="server">{{server}}</td>
    <td class="time">{{time}}</td>
    <td class="status">{{status}}</td>
    <td class="reload"><a class="btn btn-small refresh" id="{{reloadID}}"><i class="icon-repeat"></i></a></td>
  </tr>
  {{/each}}
  </tbody>
</table>